{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobNew.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 50
      },
      "minSalary": {
        "type": "string",
        "pattern": "^[0-9]+$"
      },
      "hasEquity": {
        "enum":[
          "true",
          "false"
        ]
        
      }
    },
    "additionalProperties": false,
    "required": []
  }