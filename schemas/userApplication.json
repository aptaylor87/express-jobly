{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "id": {
      "type": "string",
      "pattern": "^[0-9]+$"
    }
  },
  "additionalProperties": false,
  "required": [
    "username",
    "id"
  ]
}
